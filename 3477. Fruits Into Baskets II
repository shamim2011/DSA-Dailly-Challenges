// Approach - Brute Force Matching (Greedy with No Sorting)
// T.C - O(n^2)
// S.C - O(1)



class Solution {
    public int numOfUnplacedFruits(int[] fruits, int[] baskets) {
        int n = fruits.length;
        int unplacedCount = 0;
        for (int i=0;i<n;i++) {
            boolean placed = false;
            for(int j=0;j<n;j++){
                if(fruits[i]<=baskets[j]){
                    baskets[j] = -1;
                    placed = true;
                    break;
                }
            }
            if(!placed)
                unplacedCount++;
            
        }
        return unplacedCount;
    }
}

