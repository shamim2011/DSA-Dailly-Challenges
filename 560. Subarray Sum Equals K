// Approach -Better Brut force method
// T.C - O(n^2)
// S.C - O(1)

class Solution {
    public int subarraySum(int[] nums, int k) {
        int count=0;
        int n=nums.length;
        for(int i=0;i<n;i++){
            int sum=0;
            for(int j=i;j<n;j++){
                sum+=nums[j];
                if(sum==k){
                    count++;
                }
            }   
        }
        return count;
    }
}
