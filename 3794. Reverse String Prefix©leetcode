// Approach - Prefix Reversal Using StringBuilder
// T.C - O(n)
// S.C - O(n)


class Solution {
    public String reversePrefix(String s, int k) {
        StringBuilder sb = new StringBuilder();
        int n = s.length();
        String sub = s.substring(k,n);
        String kSub = s.substring(0,k);
        sb.append(kSub).reverse();
        sb.append(sub);
        return sb.toString();
    }
}
