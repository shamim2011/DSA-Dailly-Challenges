//Approach-1 (Brute Force)
//T.C : O(n^3)
//S.C : O(1)

class Solution {
    public int numOfSubarrays(int[] arr) {
        int odd_sum_count=0;
        int n=arr.length;
        for(int i=0;i<n;i++){
            for(int j=i;j<n;j++){
                int sum=0;
                for(int k=i;k<=j;k++){
                    sum+=arr[k];
                }
                if(sum%2!=0){
                    odd_sum_count++;
                }
            } 
        }
        return odd_sum_count;
    }
}


